    "version": "0.2.0",
    "configurations": [
       
        {
            "name": "rasa train",
            "type": "python",
            "request": "launch",
            "module": "rasa",
            "justMyCode": false,
            "args": [
                "train"
            ]
        },

        {
            "name": "rasa test",
            "type": "python",
            "request": "launch",
            "module": "rasa",
            "justMyCode": false,
            "args": [
                "test"
            ]
        },

        {
            "name": "rasa shell",
            "type": "python",
            "request": "launch",
            "module": "rasa",
            "justMyCode": false,
            "args": [
                "shell"
            ],
            "environment": {
                "DEFAULT_STREAM_READING_TIMEOUT_IN_SECONDS": 3600
            }
           
        },

        {
            "name": "rasa run server",
            "type": "python",
            "request": "launch",
            "module": "rasa",
            "justMyCode": false,
            "args": [
                "run", "-vv", "--enable-api"
            ],
            "environment": {
                "DEFAULT_STREAM_READING_TIMEOUT_IN_SECONDS": 3600
            }
           
        },

        {
            "name": "rasa run actions",
            "type": "python",
            "request": "launch",
            "module": "rasa",
            "justMyCode": false,
            "args": [
                "run", "actions", "-vv"
            ],
            "environment": {
                "DEFAULT_STREAM_READING_TIMEOUT_IN_SECONDS": 3600
            }
           
        }
    ]
}